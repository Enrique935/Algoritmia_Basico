#include <iostream>

using namespace std;

int main()
{
        //SEMANA 5 PROBLEMA SEMANAL "STARICASE"

    int n,i,j;
    cin>>n;

    for(i=1;i<=n;i++){
        for(j=1;j<=(n-i);j++){
            cout<<" ";
        }
        for(j;j<=n;j++){
            cout<<"#";
        }
        cout<<endl;
        j=0;
    }
    return 0;
}
